generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model absen_daikin {
  id     Int      @id @default(autoincrement())
  gpm_id String
  date   DateTime @default(now()) @db.Timestamp(6)
  photo  String
}

model akun_daikin {
  gpm_id String @id
  name   String
}

model lokasi_daikin {
  id        Int      @id @default(autoincrement())
  gpm_id    String
  date      DateTime @default(now()) @db.Timestamp(6)
  latitude  Decimal  @db.Decimal
  longitude Decimal  @db.Decimal
}
