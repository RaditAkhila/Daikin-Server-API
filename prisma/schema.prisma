generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model absen_daikin {
  id    Int      @id @default(autoincrement())
  waktu DateTime @default(now())
  photo String
  lat   Float
  lng   Float
}

model akun_daikin {
  fullname String  @id
  name     String
  password String
}

